  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "amd-bundle": "^1.7.4",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.12.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "should": "^13.2.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
