{
    "index": "ReadMe.md",
    "source": "./source",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": {
                    "access": ["public", "protected"],
                    "autoPrivate": false
                }
            }
        },
        {
            "name": "esdoc-external-nodejs-plugin",
            "option": {
                "enable": true
            }
        }
    ]
}
