{
  "name": "create-es-pack",
  "version": "0.4.6",
  "description": "Project generator of ECMAScript package",
  "keywords": [
    "ecmascript",
    "package",
    "project",
    "generator"
  ],
  "author": "shiy2008@gmail.com",
  "license": "LGPL-3.0",
  "homepage": "https://web-cell.tk/create-es-pack/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EasyWebApp/create-es-pack.git"
  },
  "bugs": {
    "url": "https://github.com/EasyWebApp/create-es-pack/issues"
  },
  "bin": "dist/index.js",
  "scripts": {
    "lint": "eslint source/ --fix",
    "build": "npm run lint  &&  babel source/ -d dist/ -s",
    "test": "npm run build  &&  esdoc",
    "prepublishOnly": "npm test",
    "help": "esdoc  &&  web-server docs/ -o"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.4",
    "@tech_query/node-toolkit": "^0.9.3",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "simple-git": "^1.107.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-plugin-inline-import": "^3.0.0",
    "esdoc": "^1.1.0",
    "esdoc-external-nodejs-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.15.0",
    "husky": "^1.3.1",
    "koapache": "^1.0.6"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.3.0"
          }
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-inline-import",
        {
          "extensions": [
            ".json"
          ]
        }
      ],
      "@babel/plugin-transform-runtime"
    ]
  }
}
